  },
  "dependencies": {
    "i18next-http-backend": "^2.2.0",
    "next": "13.3.0",
    "ni18n": "link:../../"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "eslint-config-next": "^13.3.0",
    "prettier": "^2.8.7"
  }
}
